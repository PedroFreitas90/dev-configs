{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#45F1C2",
          "style": "plain",
          "template": "ÓÆô {{ .UserName }} on",
          "type": "session"
        },
        {
          "foreground": "#0CA0D8",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "template": " ÔÅª {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "java",
          "style": "powerline",
          "foreground": "#DE5C19",
          "properties": {
            "home_enabled": "true"
          },
          "template": "Óâñ {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "foreground": "#14A5AE",
          "powerline_symbol": "ÓÇ∞",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} Ó≠ã {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#a9ffb4",
          "style": "plain",
          "type": "text"
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "foreground": "#D3D3D3",
          "properties": {
            "display_error": "true",
            "parse_kubeconfig": "true"
          },
          "template": " Û±Éæ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
        },
        {
          "type": "battery",
          "style": "powerline",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#FFFF00{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}Ôäï ",
          "properties": {
            "discharging_icon": "Óà± ",
            "charging_icon": "Óà¥ ",
            "charged_icon": "ÓàØ "
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "foreground": "#8800dd",
          "template": " <#8800dd>ÓÆ¢</> {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          }
        },
        {
          "type": "time",
          "style": "powerline",
          "foreground": "#007ACC",
          "properties": {
            "time_format": "üïë 15:04:05"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cd5e42",
          "style": "plain",
          "template": "Óéø ",
          "type": "root"
        },
        {
          "foreground": "#CD4277",
          "style": "plain",
          "template": "# ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
